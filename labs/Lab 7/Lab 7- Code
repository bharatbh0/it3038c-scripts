//Uploading pre-made HTML files 
//This is done use Node 
//Create a file.js and add the code below for a pre-made html template

> const HtmlWebpackPlugin = require('html-webpack-plugin')

module.exports = {
  entry: 'filename.js',
  output: {
    path: __dirname + '/dist',
    filename: 'filename.js'
  },
  plugins: [
    new HtmlWebpackPlugin()
  ]
}
